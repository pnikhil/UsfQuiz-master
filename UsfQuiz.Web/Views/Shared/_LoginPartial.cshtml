@using Microsoft.AspNet.Identity
@using UsfQuiz.Web.Controllers
@if (Request.IsAuthenticated)
{
    using (Html.BeginForm("LogOut", "Account", new { area = string.Empty }, FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
    {
    @Html.AntiForgeryToken()

    <ul class="nav navbar-nav">
        <li>
            <li>
                <a href="@Url.Action("MyAccount", "Profile")"><i class="fa fa-user"></i> MY ACCOUNT</a>
            </li>
        </li>
        <li><a href="javascript:document.getElementById('logoutForm').submit()"><i class="fa fa-sign-out"></i> LOGOUT</a></li>
    </ul>
    }
}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li><a href="@Url.Action("Login","Account")" id="loginLink"><i class="fa fa-google-plus"></i> LOGIN / REGISTER</a></li>

    </ul>
}
